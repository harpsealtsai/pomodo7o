<Window x:Class="Pomodo7o.Win"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Pomodo7o" Height="150" Width="250"
        Loaded="Window_Loaded"
        Icon="tomato.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*" />
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" >Created by Jon Fuller</Label>
        <Label Grid.Row="1" >@jon__fuller</Label>
        <Label Grid.Row="2" >http://jonfuller.codingtomusic.com</Label>
    </Grid>
</Window>
